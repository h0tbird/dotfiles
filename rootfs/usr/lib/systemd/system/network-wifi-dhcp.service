[Unit]
Description=Wi-Fi Network (dhcp)
Wants=network.target
Before=network.target
BindsTo=sys-subsystem-net-devices-wlan0.device
After=sys-subsystem-net-devices-wlan0.device

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/home/marc/bin/network wifi_dhcp start
ExecStop=/home/marc/bin/network wifi_dhcp stop

[Install]
WantedBy=multi-user.target
