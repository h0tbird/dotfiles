#------------------------------------------------------------------------------
# Github:
#------------------------------------------------------------------------------

Host github.com
  User git
  IdentityFile ~/.ssh/home.key

#------------------------------------------------------------------------------
# Qemu CoreOS:
#------------------------------------------------------------------------------

Host coreos
  HostName localhost
  Port 2222
  User core
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null

#------------------------------------------------------------------------------
# Synology:
#------------------------------------------------------------------------------

Host synology
  HostName 192.168.1.199
  Port 1315

#------------------------------------------------------------------------------
# Default:
#------------------------------------------------------------------------------

Host *
  User root
  ForwardAgent yes
  ServerAliveInterval 30
  #ControlMaster auto
  #ControlPath ~/.ssh/socket/%r@%h:%p
